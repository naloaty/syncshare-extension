{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": "off",
        "no-unused-labels": "off",
        "no-unused-vars": "off"
    }
}
